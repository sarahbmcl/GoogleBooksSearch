{
  "name": "mern",
  "homepage": "https://sarahbmcl.github.io/GoogleBooksSearch",
  "version": "1.0.0",
  "description": "GoogleBookSearch",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm install && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarahbmcl/GoogleBookSearch.git"
  },
  "engines": {
    "node": "10.15.1",
    "npm": "6.9.0"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.8.6",
    "morgan": "^1.9.1",
    "node": "^13.6.0",
    "path": "^0.12.7",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-overlays": "^2.1.0",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0"
  }
}
